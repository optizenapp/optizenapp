        "mainEntity": [
          {
            "@type": "Question",
            "name": "Why is my upsell not showing on the storefront?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Check if campaign is active (green toggle in dashboard), verify targeting rules match customer's cart, ensure theme has the Optizen app block installed, and clear browser cache to test in incognito mode.",
              "video": "https://optizenapp.com/support-docs/video-upsells/troubleshooting#video#t=30,270"
            }
          },
          {
            "@type": "Question",
            "name": "Why is the discount not applying to my upsell?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Verify discount code was created in campaign details, ensure product is eligible for the discount, check if discount has usage limits or expiration, and test the checkout process manually.",
              "video": "https://optizenapp.com/support-docs/video-upsells/troubleshooting#video#t=270,360"
            }
          },
          {
            "@type": "Question",
            "name": "Why is my video not generating properly?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Check that product has clear title and description, ensure product images are high quality, try regenerating with different voice/style options, and contact support if the issue persists.",
              "video": "https://optizenapp.com/support-docs/video-upsells/troubleshooting#video#t=360,480"
            }
          },
          {
            "@type": "Question",
            "name": "How do I test if my upsell campaign is working correctly?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Use incognito mode to simulate a new customer, add products that match your targeting rules to cart, proceed through checkout to verify upsell appears and discount applies correctly.",
              "video": "https://optizenapp.com/support-docs/video-upsells/troubleshooting#video#t=210,270"
            }
          }
