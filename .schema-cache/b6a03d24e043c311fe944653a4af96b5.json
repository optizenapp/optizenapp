      {
        "@type": "DefinedTermSet",
        "@id": "#ai-terminology-glossary",
        "name": "AI and Few-Shot Prompting Terminology",
        "hasDefinedTerm": [
          {
            "@type": "DefinedTerm",
            "@id": "#few-shot-prompting",
            "name": "Few-Shot Prompting",
            "description": "A technique that guides large language models by providing several examples or 'shots' directly within the prompt to demonstrate a specific task and desired output format",
            "sameAs": "https://en.wikipedia.org/wiki/Few-shot_learning"
          },
          {
            "@type": "DefinedTerm",
            "@id": "#in-context-learning",
            "name": "In-Context Learning",
            "description": "The ability of language models to learn and perform tasks based on examples provided within the input prompt without changing the model's internal parameters",
            "sameAs": "https://en.wikipedia.org/wiki/In-context_learning"
          },
          {
            "@type": "DefinedTerm",
            "@id": "#large-language-models",
            "name": "Large Language Models",
            "description": "AI models trained on vast amounts of text data that can understand and generate human-like text across various tasks",
            "sameAs": "https://en.wikipedia.org/wiki/Large_language_model"
          },
          {
            "@type": "DefinedTerm",
            "@id": "#prompt-engineering",
            "name": "Prompt Engineering",
            "description": "The practice of designing and optimizing input prompts to effectively communicate with and guide AI language models",
            "sameAs": "https://en.wikipedia.org/wiki/Prompt_engineering"
          },
          {
            "@type": "DefinedTerm",
            "@id": "#chain-of-thought",
            "name": "Chain-of-Thought Prompting",
            "description": "A prompting technique that includes step-by-step reasoning in examples to teach models how to think through complex problems",
            "sameAs": "https://en.wikipedia.org/wiki/Chain-of-thought_prompting"
          },
          {
            "@type": "DefinedTerm",
            "@id": "#self-consistency",
            "name": "Self-Consistency",
            "description": "A method that generates multiple reasoning paths and selects the most common answer to improve accuracy and reliability"
          },
          {
            "@type": "DefinedTerm",
            "@id": "#automatic-cot",
            "name": "Automatic Chain-of-Thought",
            "description": "An automated method for creating Chain-of-Thought prompts that reduces manual effort while maintaining performance"
          },
          {
            "@type": "DefinedTerm",
            "@id": "#set-of-mark",
            "name": "Set-of-Mark Prompting",
            "description": "A technique using special markers to guide models through long documents in structured stages"
          }
        ]
