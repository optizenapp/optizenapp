        "mainEntity": [
          {
            "@type": "Question",
            "name": "What is a good conversion rate for video upsells?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Aim for a conversion rate of 15% or higher. This means 15 out of every 100 customers who view your video upsell should make a purchase. Rates above 20% are considered excellent.",
              "video": "https://optizenapp.com/support-docs/video-upsells/analytics-optimization#video"
            }
          },
          {
            "@type": "Question",
            "name": "How often should I check my video upsell analytics?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Monitor your analytics weekly to identify trends and make timely optimizations. Daily checking may lead to over-optimization based on small sample sizes.",
              "video": "https://optizenapp.com/support-docs/video-upsells/analytics-optimization#video"
            }
          },
          {
            "@type": "Question",
            "name": "What's the difference between impressions and views?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Impressions count how many times your upsell was displayed to customers, while views count how many customers actually watched the video. Views will always be lower than impressions.",
              "video": "https://optizenapp.com/support-docs/video-upsells/analytics-optimization#video"
            }
          },
          {
            "@type": "Question",
            "name": "How do I A/B test my video upsells effectively?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Test one element at a time - either the script, voice, or visual elements. Run tests for at least 2 weeks or until you have statistical significance (typically 100+ conversions per variation).",
              "video": "https://optizenapp.com/support-docs/video-upsells/analytics-optimization#video"
            }
          },
          {
            "@type": "Question",
            "name": "What cart value threshold should I set for video upsells?",
            "acceptedAnswer": {
              "@type": "Answer",
              "text": "Start with customers who have cart values 20-50% higher than your average order value. Test different thresholds to find what works best for your specific audience and products.",
              "video": "https://optizenapp.com/support-docs/video-upsells/analytics-optimization#video"
            }
          }
        ]
